{{ define "global/header.go.tmpl" }}
<header class="header">
    <div class="header__wrap">
        {{ range .ArtistNames }}
               <a href="#" class="header__link">
					<picture>
						<source srcset="img/icons/icon-arrow.webp" type="image/webp">
						<img src="img/icons/icon-arrow.png" alt="logo" />
					</picture>{{ .Name }}
				</a>
         {{ end }}
        <a class="logo" href="#">
            <picture>
                <source srcset="/assets/img/logo.webp" type="image/webp">
                <img src="/assets/img/logo.png" alt="logo" />
            </picture>
        </a>
        </div>
</header>
{{ end }}