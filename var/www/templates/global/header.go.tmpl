{{ define "global/header.go.tmpl" }}
<header class="header">
    <div class="header__wrap">
        {{ range .ArtistNames }}
            <a href="#" class="header__link">{{ .Name }}</a>
        {{ end }}
        <a class="logo" href="#">
            <picture>
                <source srcset="/assets/img/logo.webp" type="image/webp">
                <img src="/assets/img/logo.png" alt="logo" />
            </picture>
        </a>
        <a href="#" class="header__user">
            <picture>
                <source srcset="/assets/img/icon-user.webp" type="image/webp">
                <img src="/assets/img/icon-user.png" alt="login" />
            </picture>
        </a>
    </div>
</header>
{{ end }}